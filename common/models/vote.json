{
  "name": "Vote",
  "base": "BaseEntity",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "type": {
      "type": "string",
      "required": true,
      "refcodetype": "VoteType",
      "default": "U"
    },
    "movieId": {
      "type": "string",
      "required": true,
      "unique": {
        "scopedTo": [
          "_createdBy"
        ]
      }
    }
  },
  "validations": [],
  "relations": {
    "movieInfo": {
      "type": "belongsTo",
      "model": "Movie",
      "foreignKey": "movieId"
    },
    "createdBy": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": "_createdBy",
      "primaryKey": "username"
    },
    "modifiedBy": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": "_modifiedBy",
      "primaryKey": "username"
    }
  },
  "acls": [],
  "methods": {},
  "mixins": {
    "SoftDeleteMixin": false,
    "VersionMixin": false
  }
}