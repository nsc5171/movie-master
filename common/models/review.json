{
  "name": "Review",
  "base": "BaseEntity",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true,
      "validateWhen": {
        "required": "Boolean(@i.description)"
      }
    },
    "description": {
      "type": "string"
    },
    "rating": {
      "type": "number",
      "numericality": "integer",
      "min": 1,
      "max": 5,
      "required": true
    },
    "movieId": {
      "type": "string",
      "required": true,
      "unique": {
        "scopedTo": [
          "_createdBy"
        ]
      }
    }
  },
  "validations": [],
  "relations": {
    "movieInfo": {
      "type": "belongsTo",
      "model": "Movie",
      "foreignKey": "movieId"
    },
    "createdBy": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": "_createdBy",
      "primaryKey": "username"
    },
    "modifiedBy": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": "_modifiedBy",
      "primaryKey": "username"
    }
  },
  "acls": [],
  "methods": {},
  "mixins": {
    "SoftDeleteMixin": false,
    "VersionMixin": false
  }
}